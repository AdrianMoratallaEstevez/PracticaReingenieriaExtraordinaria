@model IEnumerable<AppGestionEMS.Models.Evaluaciones>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Alumno.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Curso.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nota)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Alumno.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Curso.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nota)
        </td>
    </tr>
}

</table>
